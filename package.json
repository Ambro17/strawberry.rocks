{
  "name": "strawberry-graphql",
  "version": "1.0.0",
  "author": "Patrick Arminio <patrick.arminio@@gmail.com>",
  "engines": {
    "node": "14.x",
    "npm": "7.x"
  },
  "scripts": {
    "prebuild": "npm run codegen && npm run type-check",
    "build": "next build",
    "develop": "npm run dev",
    "dev": "next dev",
    "dev:inspect": "NODE_OPTIONS='--inspect' next dev",
    "start": "next start",
    "update-algolia": "ts-node --project scripts/tsconfig.json -r dotenv/config scripts/update-algolia.ts dotenv_config_path=.env.local",
    "format": "prettier --write \"{,!(.next|node_modules|public|coverage|.cache)/**/}*.{js,jsx,ts,tsx,json}\"",
    "test": "echo \"Error: no test specified\" && exit 1",
    "type-check": "tsc --noEmit",
    "type-check:watch": "tsc --watch --noEmit",
    "codegen": "graphql-codegen --config codegen.yml",
    "codegen:watch": "npm run codegen -- --watch",
    "lint": "eslint --cache --ext .js,.ts,.tsx ."
  },
  "keywords": [
    "strawberry-graphql",
    "strawberry",
    "graphql",
    "next.js",
    "typescript"
  ],
  "dependencies": {
    "@hashicorp/remark-plugins": "^3.2.0",
    "@mdx-js/loader": "^1.6.22",
    "@mdx-js/mdx": "^1.6.22",
    "@mdx-js/react": "^1.6.22",
    "@next/mdx": "^11.1.0",
    "@octokit/core": "^3.5.1",
    "@octokit/plugin-paginate-rest": "^2.15.1",
    "@tailwindcss/aspect-ratio": "^0.2.1",
    "algoliasearch": "^4.10.3",
    "classnames": "^2.3.1",
    "dotenv": "^10.0.0",
    "github-slugger": "^1.3.0",
    "gray-matter": "^4.0.3",
    "marked": "^2.1.3",
    "mousetrap": "^1.6.5",
    "next": "^11.1.0",
    "next-mdx-remote": "^3.0.4",
    "next-transpile-modules": "^8.0.0",
    "nprogress": "^0.2.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-helmet": "^6.1.0",
    "react-instantsearch-dom": "^6.12.1",
    "react-refractor": "^2.1.4",
    "react-use-mailchimp": "^0.4.0",
    "refractor": "^4.1.1",
    "ts-node": "^10.2.0",
    "typescript": "^4.3.5"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^2.0.1",
    "@graphql-codegen/typescript": "^2.0.0",
    "@graphql-codegen/typescript-operations": "^2.0.1",
    "@octokit/graphql-schema": "^10.60.1",
    "@trivago/prettier-plugin-sort-imports": "^2.0.2",
    "@types/classnames": "^2.3.1",
    "@types/github-slugger": "^1.3.0",
    "@types/marked": "2.0.4",
    "@types/mdx-js__react": "^1.5.4",
    "@types/mousetrap": "^1.6.8",
    "@types/node": "^16.6.1",
    "@types/nprogress": "^0.2.0",
    "@types/react": "^17.0.17",
    "@types/react-dom": "^17.0.9",
    "@types/react-helmet": "^6.1.2",
    "@types/refractor": "^3.0.2",
    "@typescript-eslint/eslint-plugin": "^4.29.1",
    "@typescript-eslint/parser": "^4.29.1",
    "autoprefixer": "^10.3.1",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-react": "^7.24.0",
    "graphql": "^15.5.1",
    "postcss": "^8.3.6",
    "prettier": "^2.3.2",
    "tailwindcss": "^2.2.7"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/strawberry-graphql/strawberry.rocks"
  }
}
